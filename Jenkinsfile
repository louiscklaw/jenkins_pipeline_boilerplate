pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        sh 'echo \'start build\''
        echo 'build done'
      }
    }
    stage('test') {
        stage('test') {
          steps {
            sh 'echo \'start build\''
            echo 'build done'
          }
        }
        stage('test1') {
          steps {
            echo 'helloworld'
          }
        }

    }
    stage('deploy') {
      steps {
        sh 'echo \'start build\''
        echo 'build done'
      }
    }
  }
}
